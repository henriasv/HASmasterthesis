month = jun,
number = {11},
pages = {3476--3495},
title = {{Kinetic simulation of methane hydrate formation and dissociation in porous media}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0009250905009607},
volume = {61},
year = {2006}
}
